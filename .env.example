# Copy this file to .env and fill in your actual API keys
# API Keys - DO NOT COMMIT TO VERSION CONTROL
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Database Configuration
# Set to 'true' to use PostgreSQL instead of MongoDB
USE_POSTGRESQL=false

# PostgreSQL Connection (if USE_POSTGRESQL=true)
# For macOS Homebrew installations, use your macOS username (no password needed)
# Example: postgresql://jaypound@localhost/ftp_media_sync
DATABASE_URL=postgresql://YOUR_USERNAME@localhost/ftp_media_sync

# MongoDB Connection (if USE_POSTGRESQL=false)
MONGODB_URI=mongodb://localhost:27017/
MONGODB_DATABASE=castus